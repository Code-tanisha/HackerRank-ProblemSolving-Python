import math

import random
import re
import sys
import os

# Complete the compareTriplets function below.
def compareTriplets(a, b):
    count1 = 0
    count2 = 0
    
    for i in range(3):
            if a[i] > b[i]:
                count1 += 1
                
            elif b[i] > a[i]:
                count2 += 1
    return (count1, count2)
